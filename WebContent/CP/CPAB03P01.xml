	<Script><![CDATA[/***************************************************************************************************
* FormID(명) : 사건담당선택팝업
* 작성자     : 이용재
* 작성일     : 2013-02-13 : 최초 생성
* 기능설명   : 사건담당선택 팝업 관리
* 변경로그
* Date             Ver          Name        Description
* ---------------  ------   ----------  ---------------------------
* 2013-02-13       V1.0                  initial ver.
***************************************************************************************************/

/**************************************************************************************************
  * 공통 Script Include
***************************************************************************************************/
#include "lib::lib_global.js";
#include "lib::lib_comm.js";
#include "lib::lib_grid.js";
#include "lib::lib_platform.js";
#include "lib::lib_popup.js";
#include "lib::lib_msg.js";
#include "lib::lib_ext.js";

/**************************************************************************************************
  * 화면 변수 선언부
***************************************************************************************************/

var svcID			= "";		
var url   			= "";
var inDatasets  	= "";
var outDatasets 	= "";
var argument    	= "";
var callbackFunc 	= "";
 

/**************************************************************************************************
  * 공통 Function 처리 부분
***************************************************************************************************/

//  1-1.화면 셋팅 함수 
//  1-2.조회 함수     
//  1-3.Transaction 후 처리 해야 할 내용
//  1-4.창닫기

//*------------------------------------------------------------------------
//* 함수설명     : 1.1 화면셋팅 함수 
//* 함수유형     :
//* parameter    :
//* return value : 없음
//* 비고      
//*------------------------------------------------------------------------
function fn_initForm()
{

	S_CHG_NM.Text = P_CHG_NM; //부모값
	
	fn_search();
}

//*------------------------------------------------------------------------------------------------
//* 함수설명     : 1.2 조회 함수 
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//* 비고         :
//*-----------------------------------------------------------------------------------------------
function fn_search()
{
	PDS_TIEUPCHG_LST.ClearData();
	
	svcID			= "CPAB03P01.selectTieUpChgPopup";
	url   			= "service::common/select_mi.do";
	inDatasets  	= "";
	outDatasets 	= "PDS_TIEUPCHG_LST=cc.CCTieUpDAO.selectTieUpChgPopup";
	argument    	= " _sqlName=cc.CCTieUpDAO.selectTieUpChgPopup"
					+ " S_BRCH_ID=" + quote(P_BRCH_ID)		// 부모창에서 받은값	
					+ " S_TUB_NM=" + quote(S_TUB_NM.Text)
					+ " S_CHG_NM=" + quote(S_CHG_NM.Text)	
					;
	
	
	callbackFunc 	= "gfn_callback";

	gfn_showProgressWindow(true);
	gfn_Transaction(svcID, url, inDatasets, outDatasets, argument, callbackFunc);	 
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 1.3 Transaction 호출후 결과값 확인 (gfn_callback에서 fn_callback을 호출한다.)
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//* 비고         :
//*-----------------------------------------------------------------------------------------------
function fn_callback(svcID, errorCode, errorMsg)
{
	gfn_showProgressWindow(false);
	
	switch(svcID) 
	{
		case "CPAB03P01.selectTieUpChgPopup" :
			if(PDS_TIEUPCHG_LST.rowcount == 1)
			{
				fn_set_tieup_info(0);
			}
			else
			{
				GRD_TIEUPCHG_LST.SetFocus();
			}

		break;	

		case "CPAB03P01.saveTmpTieUpChgPopup" :
			fn_search();

		break;				
		
		
		default:
			 // alert("기타"); 
	}
}


//*------------------------------------------------------------------------------------------------
//* 함수설명     : 1.4 창닫기
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function fn_close()
{
	close(false);
}



/**************************************************************************************************
  * 사용자 정의  Function 처리 부분
***************************************************************************************************/

//*------------------------------------------------------------------------------------------------
//* 함수설명     : 코드 값 setting
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function fn_set_tieup_info(c_row)
{	

	// 부모창에 넘기기
	
	var retArr = Array();
	
	retArr[0] = PDS_TIEUPCHG_LST.GetColumn(c_row,"BRCH_ID");
	retArr[1] = PDS_TIEUPCHG_LST.GetColumn(c_row,"TUB_SEQ");
	retArr[2] = PDS_TIEUPCHG_LST.GetColumn(c_row,"TUB_NM");
	retArr[3] = PDS_TIEUPCHG_LST.GetColumn(c_row,"CHG_SEQ");
	retArr[4] = PDS_TIEUPCHG_LST.GetColumn(c_row,"CHG_NM");
	retArr[5] = PDS_TIEUPCHG_LST.GetColumn(c_row,"TUB_TEL");
	retArr[6] = PDS_TIEUPCHG_LST.GetColumn(c_row,"TUB_FAX");
	retArr[7] = PDS_TIEUPCHG_LST.GetColumn(c_row,"HNA_CHG_USR");
	retArr[8] = PDS_TIEUPCHG_LST.GetColumn(c_row,"HNA_CHG_USR_NM");
	retArr[9] = PDS_TIEUPCHG_LST.GetColumn(c_row,"DOC_CARY");
	retArr[10] = PDS_TIEUPCHG_LST.GetColumn(c_row,"BIZ_FILE_SAVE_NM");
	retArr[11] = PDS_TIEUPCHG_LST.GetColumn(c_row,"AFF_REG_NOTE");
	retArr[12] = PDS_TIEUPCHG_LST.GetColumn(c_row,"CLT_OPN_YN");
	retArr[13] = PDS_TIEUPCHG_LST.GetColumn(c_row,"CLT_OPN_HP");
	retArr[14] = PDS_TIEUPCHG_LST.GetColumn(c_row,"SIN_GBN");
	retArr[15] = PDS_TIEUPCHG_LST.GetColumn(c_row,"SIN_NM");
			  
	close(retArr);	

}





/**************************************************************************************************
  * 공통 Event 처리 부분
***************************************************************************************************/
//  2-1.Load 부분
//  2-2.UnLoad 부분
//  2-3.조회 버튼 클릭  
//  2-4.확인 버튼 클릭
//  2-5.닫기 버튼 클릭  

//*------------------------------------------------------------------------------------------------
//* 함수설명     : 화면 Load 완료시 실행 스크립트 
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function CPAB03P01_OnLoadCompleted(obj)
{
	// 공통 init
	gfn_OnInit();
	
    fn_initForm();	
}


//*------------------------------------------------------------------------------------------------
//* 함수설명     : 화면 UnLoad 완료시 실행 스크립트 
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function CPAB03P01_OnUnloadCompleted(obj)
{
	gfn_OnBeforeExit();
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 조회
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function btn_search_OnClick(obj)
{
	fn_search();
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 확인 
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function btn_confirm_OnClick(obj)
{
	if(CHK_TMP_TUB.Value == '1')
	{
		// 임시제휴사 등록
		if(length(S_TUB_NM.Text) == 0 || length(S_CHG_NM.Text) == 0)
		{
			gfn_popup_message("modal", "error", "등록할 임시제휴사명과 담당자명을  입력하세요.", "확인");
			S_BND_NM.SetFocus();
		}
		else
		{
			if(gfn_popup_message("modal", "confirm", S_TUB_NM.Text + "를 제휴사로 등록하시겠습니까?", "확인")) 
			{
			
				svcID			= "CPAB03P01.saveTmpTieUpChgPopup";
				url   			= "service::common/select_mi.do";
				inDatasets  	= "";
				outDatasets 	= "";
				argument    	= " _sqlName=cc.CCTieUpDAO.saveTmpTieUpChgPopup"
										+ " BRCH_ID=" + quote(GDS_USER_INFO.BRCH_ID)
										+ " TUB_NM=" + quote(S_TUB_NM.Text)
										+ " CHG_NM=" + quote(S_CHG_NM.Text)
										+ " OPERATOR=" + quote(GDS_USER_INFO.USR_ID);		
				callbackFunc 	= "gfn_callback";	
			
			
				gfn_showProgressWindow(true);
				gfn_Transaction(svcID, url, inDatasets, outDatasets, argument, callbackFunc);	 
			}				
		}
		
		
		
	}
	else
	{
		fn_set_tieup_info(PDS_TIEUPCHG_LST.currow);
	}
	
	
	
	
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 창닫기
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function btn_close_OnClick(obj)
{
	fn_close();
}



/**************************************************************************************************
  * 사용자 정의  Event 처리 부분
***************************************************************************************************/

//*------------------------------------------------------------------------------------------------
//* 함수설명     : 그리드 헤더클릭
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function GRD_TIEUPCHG_LST_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{

	gfn_GridSort(GRD_TIEUPCHG_LST, PDS_TIEUPCHG_LST, nCell);	
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 그리드 더블클릭
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function GRD_TIEUPCHG_LST_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	fn_set_tieup_info(nRow);
}

//*------------------------------------------------------------------------------------------------
//* 함수설명     : 그리드 엔터
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------
function GRD_TIEUPCHG_LST_OnEnterDown(obj,nRow,nCell,strVal,nPivotIndex)
{
	fn_set_tieup_info(nRow);
}



//*------------------------------------------------------------------------------------------------
//* 함수설명     : 조회 엔터키 
//* 함수유형     : 
//* parameter    : 
//* return value : 없음
//*-----------------------------------------------------------------------------------------------

function S_CHG_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if( nChar == 13 )
	{
		fn_search();
	}	
}

]]></Script>
